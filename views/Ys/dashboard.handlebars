<div class="dashboard">
    <div class="title-container">
        <h2>Perfil</h2>
        <a href="/Ys/add" class="btn">Criar Y</a>
    </div>
    <h3>Seus Y</h3>
    <ul>
        {{#each Ys}}
            <li>
                <span class="label">{{this.title}}</span>
                <span class="actions">
                    <a href="/Ys/edit/{{this.id}}" class="btn">Editar</a>
                    <form action="/Ys/remove" method="POST">
                        <input type="hidden" name="id" value="{{this.id}}">
                        <input type="submit" value="Excluir" class="btn">
                    </form>
                </span>
            </li>
        {{/each}}
    </ul>
</div>